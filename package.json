{
  "name": "dashboard",
  "version": "0.0.1",
  "main": "src/main.tsx",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "ts-node --files ./src/server/index.ts",
    "build": "yarn install && parcel build ./src/index.html",
    "serve": "NODE_ENV=production TS_NODE_FILES=true ts-node ./src/server/index.ts",
    "serve-forever": "NODE_ENV=production TS_NODE_FILES=true forever start -c 'node -r ts-node/register' ./src/server/index.ts",
    "host": "yarn build && yarn serve",
    "host-forever": "yarn build && yarn serve-forever"
  },
  "dependencies": {
    "dayjs": "^1.7.7",
    "express": "^4.16.4",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "request": "^2.88.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "styled-components": "^3.4.9",
    "styled-reset": "^1.3.5",
    "styles": "^0.2.1",
    "ts-node": "^7.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/parcel-bundler": "^1.10.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/request": "^2.47.1",
    "@types/socket.io": "^1.4.38",
    "@types/socket.io-client": "^1.4.32",
    "parcel-bundler": "^1.10.3",
    "typescript": "^3.1.1"
  }
}
